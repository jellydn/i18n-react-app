    "react-dom": "17.0.2",
    "typesafe-i18n": "5.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/express": "4.17.13",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "cypress": "9.5.2",
    "eslint": "8.12.0",
    "eslint-config-productsway": "0.1.11",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "typescript": "4.6.3",
    "vite": "2.8.6"
